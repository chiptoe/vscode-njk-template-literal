{
    "name": "vscode-njk-template-literal",
    "displayName": "vscode-njk-template-literal",
    "description": "Syntax highlighting for sql in template literals",
    "version": "0.1.0",
    "publisher": "forbeslindesay",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ForbesLindesay/vscode-njk-template-literal.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.js",
                    "source.js.jsx",
                    "source.jsx",
                    "source.ts",
                    "source.tsx"
                ],
                "scopeName": "inline.lit-njk",
                "path": "./syntaxes/lit-njk.json",
                "embeddedLanguages": {
                    "meta.embedded.block.sql": "nunjucks"
                }
            }
        ]
    }
}